.tabBar{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  background-color: #fff;
  display: flex;
  z-index: 888;
}
.imagess{
  width: 80rpx;
  height: 80rpx;
  padding: 5rpx;
}
.bar-item{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.menuBar-image{
  position: fixed;
  bottom:5rpx;
  left: 335rpx;
  width: 80rpx;
  height:80rpx;
  transform: rotate(-180deg);
  transition: all 0.5s;
  z-index: 880;
}
.menuBar-image-left{
  transform: rotate(180deg);
}
.showImg1{
  bottom:140rpx;
  left: 120rpx;
  transform: rotate(0deg);
  transition: all 0.5s;
}
.showImg2{
  bottom:140rpx;
  left: 260rpx;
  transform: rotate(0deg);
  transition: all 0.5s;
}
.showImg3{
  bottom:140rpx;
  left: 400rpx;
  transform: rotate(0deg);
  transition: all 0.5s;
}
.showImg4{
  bottom:140rpx;
  left: 540rpx;
  transform: rotate(0deg);
  transition: all 0.5s;
}


/*
下拉加载
*/

.page {
  position: absolute;
  width: 100%;
  height: 100%;
}

.main {
  width: 100%;
  min-height: 100%;
}

.main .sticky {
  padding-bottom: 100rpx;
}

.footer {
  position: relative;
}

.infinite-scroll-item {
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: solid 1rpx #eee;
  text-align: center;
}

.infinite-scroll-item:first-of-type {
  border-top: solid 1rpx #eee;
}