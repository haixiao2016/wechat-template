.dialog-bg{
  position: fixed;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 1000;
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  opacity: 0;
}
.dialog-container{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 16px; 
  line-height: 20px;
  min-width: 200rpx;
  max-height:800rpx;
  overflow:hidden;
  background:rgba(0,0,0,0.8);
  border-radius: 10rpx; 
  color: white;
  padding: 20rpx;
  text-align: center;
  transform: scale(0); 
}
.dialog-context{
  max-width:300rpx;
  /* margin-top:20rpx; */
}
.show-alert{
  animation: showAnimate 0.3s;
}
.hide-alert{
  animation: hideAnimate 0.3s;
}
@keyframes showAnimate {
  0% {
    transform: scale(0.7);
    -webkit-transform: scale(0.7); }
  45% {
    transform: scale(1.05);
    -webkit-transform: scale(1.05); }
  80% {
    transform: scale(0.95);
    -webkit-transform: scale(0.95); }
  100% {
    transform: scale(1);
    -webkit-transform: scale(1); }
}
@keyframes hideAnimate {
  0% {
    transform: scale(1);
    -webkit-transform: scale(1); }
  100% {
    transform: scale(0);
    -webkit-transform: scale(0); }
}